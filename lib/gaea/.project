(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: gaea           description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "gaea"}
        (<Project-Directory>                        directory: {Directory :context "src" "gaea" "geometry"}
          (<Jazz-File>         name: _geometry      source: {File :context "src" "gaea" "geometry" "_geometry.jazz"})
          (<Jazz-File>         name: classes        source: {File :context "src" "gaea" "geometry" "classes.jazz"})
          (<Jazz-File>         name: syntax         source: {File :context "src" "gaea" "geometry" "syntax.jazz"})
          (<Jazz-File>         name: base           source: {File :context "src" "gaea" "geometry" "base.jazz"}))
        (<Jazz-File>           name: homogeneous    source: {File :context "src" "gaea" "homogeneous.jazz"})
        (<Jazz-File>           name: lookat         source: {File :context "src" "gaea" "lookat.jazz"})
        (<Jazz-File>           name: math           source: {File :context "src" "gaea" "math.jazz"})
        (<Jazz-File>           name: movement       source: {File :context "src" "gaea" "movement.jazz"})
        (<Jazz-File>           name: parallelepiped source: {File :context "src" "gaea" "parallelepiped.jazz"})
        (<Jazz-File>           name: plane          source: {File :context "src" "gaea" "plane.jazz"})
        (<Jazz-File>           name: install        source: {File :context "src" "gaea" "install.jazz"})
        (<Jazz-File>           name: types          source: {File :context "src" "gaea" "types.jazz"})
        (<Jazz-File>           name: dye            source: {File :context "src" "gaea" "dye.jazz"})
        (<Jazz-File>           name: literals       source: {File :context "src" "gaea" "literals.jazz"})
        (<Jazz-File>           name: syntax         source: {File :context "src" "gaea" "syntax.jazz"})
        (<Project-Directory>                        directory: {Directory :context "src" "gaea" "validate"}
          (<Project-Directory>                      directory: {Directory :context "src" "gaea" "validate" "suite"}
            (<Jazz-File>       name: geometry       source: {File :context "src" "gaea" "validate" "suite" "geometry.jazz"}))
          (<Jazz-File>         name: _validate      source: {File :context "src" "gaea" "validate" "_validate.jazz"})
          (<Jazz-File>         name: run            source: {File :context "src" "gaea" "validate" "run.jazz"}))
        (<Jazz-File>           name: shader         source: {File :context "src" "gaea" "shader.jazz"})
        (<Jazz-File>           name: mesh           source: {File :context "src" "gaea" "mesh.jazz"})
        (<Jazz-File>           name: buffer         source: {File :context "src" "gaea" "buffer.jazz"})
        (<Jazz-File>           name: shaders        source: {File :context "src" "gaea" "shaders.jazz"})
        (<Jazz-File>           name: programs       source: {File :context "src" "gaea" "programs.jazz"})
        (<Jazz-File>           name: material       source: {File :context "src" "gaea" "material.jazz"})
        (<Jazz-File>           name: generate       source: {File :context "src" "gaea" "generate.jazz"})
        (<Jazz-File>           name: program        source: {File :context "src" "gaea" "program.jazz"}))))))
